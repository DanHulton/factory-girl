{
  "name": "factory-girl",
  "description": "A factory library for Node.js and JavaScript inspired by factory_girl",
  "main": "./index",
  "author": "Simon Wade",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "import/no-mutable-exports": 0,
      "max-len": ["error", 80]
    }
  },
  "version": "4.0.0-beta",
  "keywords": [
    "factory",
    "test",
    "bdd",
    "tdd",
    "fixture"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/aexmachina/factory-girl.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/aexmachina/factory-girl/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/aexmachina/factory-girl/raw/master/LICENSE"
    }
  ],
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chance": "^1.0.3",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^2.9.25",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^5.1.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.2.5",
    "rollup": "^0.27.0",
    "rollup-plugin-babel": "^2.5.0",
    "should": "^6.0.3",
    "sinon": "^2.0.0-pre",
    "sinon-chai": "https://github.com/sjmulder/sinon-chai.git#pr/sinon-2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src tests tools",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-register tests",
    "test:cover": "NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive tests",
    "build": "node tools/build"
  },
  "files": [
    "lib",
    "test",
    "index.js"
  ]
}
